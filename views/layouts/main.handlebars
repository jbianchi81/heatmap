			async function plotDensPronoBySerie(chart, calibrados, status, formdict, url, maxvalue) {
				chart.yAxis.formatter = {
					
				}
				chart.update({
					tooltip: {
						formatter: function () {
							const item = y_axis_items[this.series.yAxis.categories[this.point.y]]
							return `<b>${item.id} - ${item.nombre} en ${item.serie.estacion_nombre} (${item.serie.estacion_id}) - ${item.serie.var_var} [${item.serie.unit_abrev}]</b> tiene <br><b>${this.point.value}</b> registros en <br><b>${this.series.xAxis.categories[this.point.x]}</b>`;
						}
					}
				})

				var heatmapdata = []
				var i = 0
				for(const calibrado of calibrados) {
					var cal_id = calibrado.id
					for(const serie of calibrado.series) {
						var thisformdict = {...formdict}
						thisformdict.cal_id = cal_id
						thisformdict.include_timestamps = true
						thisformdict.series_id = serie.series_id
						thisformdict.cor_id = serie.cor_id
						thisformdict.tipo = getTipo(serie.series_table)
						var querystring = Object.keys(thisformdict).map(key =>
							`${key}=${thisformdict[key]}`
						).join("&")
						console.log(querystring)
						try {
							var data = await $.get(url + "?" + querystring)
						} catch(e) {
							alert("get densidad failed!: " + e.toString());
							// chart.hideLoading();
							document.body.style.cursor = "default";
							continue
						}
						if(!data || !data.values || !Array.isArray(data.values)) {
							// chart.hideLoading();
							alert("get densidad error! Not found")
							// document.body.style.cursor = "default";
							continue
						}
						heatmapdata = heatmapdata.concat(data.values.map((it,j)=>{
							if(it > maxvalue) {
								maxvalue = it
							}
							return [j,i,it]
						}))
						var chartwidth = $(window).width()
						if(i==0) {  
							chart.series[0].setData(heatmapdata)
