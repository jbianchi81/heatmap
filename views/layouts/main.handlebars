				for(const cal of calibrados) {
					if (formdict.get_series) {
						 for(const serie of cal.series) {
							const value = `${cal.id} - ${serie.series_id} - ${serie.cor_id}`
							const item = {
								id: cal.id,
								nombre: cal.nombre,
								serie: serie
							}
							y_axis_items[value] = item
							y_axis_categories.push(value)
						} 
					} else {
						const value = `${cal.id} - ${cal.nombre}`
						y_axis_items[value] = cal
						y_axis_categories.push(value)
					}
